{
  "name": "@acme/nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "dev": "pnpm with-env next dev --turbopack",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@acme/api": "workspace:*",
    "@acme/auth": "workspace:*",
    "@acme/db": "workspace:*",
    "@acme/external-services": "workspace:*",
    "@acme/ui": "workspace:*",
    "@acme/validators": "workspace:*",
    "@ai-sdk/openai": "^3.0.2",
    "@heroicons/react": "^2.2.0",
    "@t3-oss/env-nextjs": "^0.13.6",
    "@tanstack/react-form": "^1.27.3",
    "@tanstack/react-form-nextjs": "^1.27.3",
    "@tanstack/react-query": "catalog:",
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "@trpc/tanstack-react-query": "catalog:",
    "ai": "^6.0.6",
    "html5-qrcode": "^2.3.8",
    "next": "16.0.10",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "superjson": "2.2.2",
    "use-debounce": "^10.0.5",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tailwind-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@types/node": "^22.15.29",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "catalog:",
    "jiti": "^2.4.2",
    "prettier": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@acme/prettier-config",
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.7",
      "@types/react-dom": "19.2.3"
    }
  }
}
